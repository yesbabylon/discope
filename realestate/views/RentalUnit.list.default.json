{
    "name": "Rental Unit List",
    "description": "This view is intended for displaying the list of rental units.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "layout": {
        "items": [
            {
                "type": "field",
                "value": "name",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "type",
                "width": "20%"
            },
            {
                "type": "field",
                "value": "is_accomodation",
                "width": "20%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "capacity",
                "width": "20%"
            }
        ]
    }
}