{
    "name": "Booking Line Price Adapters",
    "description": "This view is intended for displaying the list of booking line price adapters.",
    "access": {
        "groups": ["booking.default.user"]
     },
    "layout": {
        "items": [
            {
                "type": "field",   
                "label": "Booking",         
                "value": "booking_id",
                "width": "20%"
            },
            {
                "type": "field",     
                "label": "Booking Line",       
                "value": "booking_line_id",
                "width": "20%"
            },
            {
                "type": "field",            
                "value": "type",
                "width": "20%"
            },
            {
                "type": "field",            
                "value": "value",
                "width": "20%"
            },
            {
                "type": "field",    
                "label": "Discount",        
                "value": "discount_id",
                "width": "20%"
            }
        ]
    }
}