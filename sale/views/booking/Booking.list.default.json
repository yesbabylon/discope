{
    "name": "Booking list",
    "description": "This view is intended for displaying the list of bookings.",
    "access": {
        "groups": ["booking.default.user"]
     },
    "layout": {
        "items": [
            {
                "type": "field",
                "value": "name",
                "label": "reference",
                "width": "5%"
            },
            {
                "type": "field",
                "label": "Customer",
                "value": "customer_id",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "From",
                "value": "date_from",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "To",
                "value": "date_to",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "Type",
                "value": "type_id",
                "width": "15%"
            },
            {
                "type": "field",
                "value": "status",
                "width": "15%"
            }
        ]
    }
}