{
    "name": "Autosale List",
    "description": "",
    "access": {
        "groups": ["sale.default.user"]
     },
    "layout": {
        "groups": [
            {
                "sections": [
                    {
                        "label": "General Info",
                        "id": "section.autosale_list_section",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "50%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "name",
                                                "width": "100%",
                                                "widget": {
                                                    "heading": true
                                                }
                                            },
                                            {
                                                "type": "field",
                                                "value": "date_from",
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "value": "date_to",
                                                "width": "50%"
                                            },
                                            {
                                                "type": "field",
                                                "label": "List Category",
                                                "value": "autosale_list_category_id",
                                                "width": "50%"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "label": "Autosale Lines",
                        "id": "section.autosale_lines_section",
                        "rows": [
                            {
                                "columns": [
                                    {
                                        "width": "100%",
                                        "items": [
                                            {
                                                "type": "field",
                                                "value": "autosale_lines_ids",
                                                "width": "100%"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}